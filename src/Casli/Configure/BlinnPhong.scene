Camera:
	Eye 0 0 -2.3
	Focus 0.0 0.0 0.0
	Up 0.0 1.0 0.0

Frustum:
	Fovy 45.0
	Near 0.1
	Nar 100.0
	
DirectionalLight:
	offset 0
	LightDir 1.0 1.0 -1.0
	Lightcolor 1.0 1.0 1.0
	
Entity:
	Path ../src/Casli/Model/diablo3_pose/diablo3_pose.obj
	VertexShader LambertVS
	PixelShader BlinnPhongPS
	Sampler 0 0
	translate 0.0 0.0 0.0
	rotate 0.0 1.0 0.0 0.0
	scale 1 1 1
	Topology 1
	Cullface: back
	AlphaBlend false
		
