Camera:
	Eye 0 0 -2.5
	Focus 0.0 0.0 0.0
	Up 0.0 1.0 0.0

Frustum:
	Fovy 45.0
	Near 0.1
	Nar 100.0

DirectionalLight:
	offset 0
	LightDir -1.0 0 -1
	Lightcolor 1.0 1.0 1.0
	
Plane:
	VertexShader LambertVS
	PixelShader SampleTexturePS
	TexturePath ../resource/Image/vase_plant.png
	MipMap 1
	Index 0
	Sampler 0 0
	translate 0.0 0.0 0.0 
	rotate 0.0 1.0 0.0 0.0
	scale 1.0 1.0 1.0
	Cullface back
	Mipmap: false
	AlphaBlend true
	Light false
	
Entity:
	Path ../resource/Model/mary/Marry.obj
	VertexShader LambertVS
	PixelShader LambertPS
	Sampler 0 0
	translate 0.0 -1.5 0.0
	rotate 0.0 1.0 0.0 0.0
	scale 0.5 0.5 0.5
	Topology 1
	Cullface back
	Mipmap false
	AlphaBlend false
	Light true
	
Entity:
	Path ../resource/Model/floor/floor.obj
	VertexShader LambertVS
	PixelShader SampleTexturePS
	Sampler 0 0
	translate 0.0 0.25 0.0
	rotate 0.0 1.0 0.0 0.0
	scale 1 1 1
	Topology 1
	Cullface back
	Mipmap false
	AlphaBlend false
	Light false
